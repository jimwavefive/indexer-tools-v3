{
  "name": "@indexer-tools/shared",
  "version": "4.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./graphql": "./dist/graphql/index.js",
    "./graphql/*": "./dist/graphql/*.js",
    "./math/*": "./dist/math/*.js",
    "./types/*": "./dist/types/*.js",
    "./config/*": "./dist/config/*.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "graphql": "^16.6.0",
    "graphql-request": "^7.1.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vitest": "^4.0.0"
  }
}
